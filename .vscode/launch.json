{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/pacman.py",
            "args": [
                "-l",
                "mediumMaze",
                "-p",
                "SearchAgent",
                "-a",
                "fn=ga"
            ],
            "console": "integratedTerminal"
        }
    ]
}